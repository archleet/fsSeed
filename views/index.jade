        ul#stats-collapse.panel-collapse.collapse
          li
            a(href='#') Stats
          li
            a(href='#') Charts
          li
            a(href='#') Realtime
      li.panel
        a.accordion-toggle.collapsed(data-toggle='collapse', data-parent='#side-nav', href='#ui-collapse')
          i.fa.fa-magic
          span.name User Interface
        ul#ui-collapse.panel-collapse.collapse
          li
            a(href='#') Buttons
          li
            a(href='#') Dialogs
          li
            a(href='#') Icons
          li
            a(href='#') Tabs
          li
            a(href='#') Accordion
      li.panel
        a.accordion-toggle.collapsed(data-toggle='collapse', data-parent='#side-nav', href='#components-collapse')
          i.fa.fa-bookmark-o
          span.name Components
        ul#components-collapse.panel-collapse.collapse
          li
            a(href='component_calendar.html') Calendar
          li
            a(href='component_maps.html') Maps
          li
            a(href='component_gallery.html') Gallery
          li
            a(href='component_fileupload.html') Fileupload
          li
            a(href='component_bootstrap.html')
              | Bootstrap
              span.label.label-important new
          li
            a(href='component_list_groups.html')
              | List Groups
              span.label.label-important new
      li.panel
        a.accordion-toggle.collapsed(data-toggle='collapse', data-parent='#side-nav', href='#tables-collapse')
          i.fa.fa-table
          span.name Tables
        ul#tables-collapse.panel-collapse.collapse
          li
            a(href='tables_static.html') Static
          li
            a(href='tables_dynamic.html') Dynamic
      li
        a.accordion-toggle.collapsed(data-toggle='collapse', data-parent='#side-nav', href='#grid-collapse')
          i.fa.fa-bars
          span.name Grid
        ul#grid-collapse.panel-collapse.collapse
          li
            a(href='grid.html') Basic
          li
            a(href='grid_live.html')
              | Live
              span.label.label-important new
      li.panel
        a.accordion-toggle.collapsed(data-toggle='collapse', data-parent='#side-nav', href='#special-collapse')
          i.fa.fa-asterisk
          span.name Special
        ul#special-collapse.panel-collapse.collapse
          li
            a(href='special_search.html') Search
          li
            a(href='special_invoice.html') Invoice
