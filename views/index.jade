          i.fa.fa-edit
          span.name Posts
        ul#posts-collapse.panel-collapse.collapse
          li
            a(href='/addPost') Add Post
          li
            a(href='#') Article
          li
            a(href='#') Elements
          li
            a(href='#') Validation
          li
            a(href='#') Wizard
      li.panel
